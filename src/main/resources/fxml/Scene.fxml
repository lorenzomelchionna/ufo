<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polito.tdp.ufo.FXMLController">
   <center>
      <VBox prefHeight="57.0" prefWidth="193.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="29.0" prefWidth="172.0">
               <children>
                  <Label text="Forma">
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="boxForma">
                     <HBox.margin>
                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                     </HBox.margin>
                  </ComboBox>
                  <Button mnemonicParsing="false" onAction="#doConta" text="Conta">
                     <HBox.margin>
                        <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="txtAvvistamenti" prefHeight="17.0" prefWidth="177.0" text="Numero di Avvistamenti">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </center>
</BorderPane>
